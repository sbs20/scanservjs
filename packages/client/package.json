{
  "name": "scanservjs",
  "version": "2.17.1",
  "description": "scanservjs is a simple web-based UI for SANE which allows you to share a scanner on a network without the need for drivers or complicated installation.",
  "author": "Sam Strachan",
  "scripts": {
    "serve": "nodemon --exec 'vue-cli-service serve'",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\""
  },
  "main": "server/server.js",
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "core-js": "^3.11.2",
    "vue": "^2.6.12",
    "vue-i18n": "^8.24.4",
    "vue-router": "^3.5.1",
    "vuetify": "^2.4.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "deepmerge": "^4.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "nodemon": "^2.0.7",
    "sass": "^1.32.12",
    "sass-loader": "^10.0.2",
    "vue-advanced-cropper": "0.16.10",
    "vue-cli-plugin-i18n": "~2.1.0",
    "vue-cli-plugin-vuetify": "~2.4.0",
    "vue-template-compiler": "^2.6.12",
    "vue-toastr": "^2.1.2",
    "vuetify-loader": "^1.7.2"
  },
  "browserslist": [
    "> 0.2%",
    "last 2 versions",
    "not dead"
  ],
  "license": "GPL-2.0",
  "nodemonConfig": {
    "watch": [
      "../server/src"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sbs20/scanservjs"
  }
}
