{
  "name": "scanservjs-api",
  "version": "2.27.0",
  "description": "scanservjs-api is a REST based API to control your scanner.",
  "scripts": {
    "lint": "gulp lint",
    "build": "gulp build",
    "package": "gulp package",
    "serve": "npm run build && node ./src/server.js",
    "serve-nodemon": "nodemon -e js,yml --watch src --exec 'npm run build && node ./src/server.js'",
    "test": "mocha"
  },
  "main": "server/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sbs20/scanservjs"
  },
  "devDependencies": {
    "eslint": "^7.2.5",
    "gulp": "4.0.2",
    "gulp-chmod": "^3.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^6.0.0",
    "gulp-filter": "^7.0.0",
    "gulp-gzip": "^1.4.2",
    "gulp-tar": "^3.1.0",
    "merge-stream": "^2.0.0",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7"
  },
  "author": "Sam Strachan",
  "license": "GPL-2.0",
  "dependencies": {
    "adm-zip": "^0.5.5",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.1",
    "loglevel": "^1.7.0",
    "loglevel-plugin-prefix": "^0.8.4",
    "mv": "^2.1.1",
    "semver": "^7.3.8",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0"
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ]
